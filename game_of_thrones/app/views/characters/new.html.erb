<h1> Add a New Character </h1>

<div class="form">
<%= form_for [@house, @character] do |f| %>

  <h3><%= f.label :name %></h3>
  <p><%= f.text_field :name %></p>

  <h3><%= f.label :bio %></h3>
  <p><%= f.text_area :bio %></p>

  <h3><%= f.label :photo_url %></h3>
  <p><%= f.text_field :photo_url %></p>

  <p><%= f.submit %></p>

<% end %>
</div>
